type Verifier = 
 service {
   addMyProfile: (StudentProfile) -> (Result_1);
   deleteMyProfile: () -> (Result_1);
   seeAProfile: (principal) -> (Result_2);
   test: (principal) -> (TestResult);
   updateMyProfile: (StudentProfile) -> (Result_1);
   verifyOwnership: (principal, principal) -> (bool);
   verifyWork: (principal, principal) -> (Result);
 };
type TestResult = 
 variant {
   err: TestError;
   ok;
 };
type TestError = 
 variant {
   UnexpectedError: text;
   UnexpectedValue: text;
 };
type StudentProfile = 
 record {
   graduate: bool;
   name: text;
   team: text;
 };
type Result_2 = 
 variant {
   err: text;
   ok: StudentProfile;
 };
type Result_1 = 
 variant {
   err: text;
   ok;
 };
type Result = 
 variant {
   err: text;
   ok: bool;
 };
service : () -> Verifier
